{
  "canisters": {
    "backend": {
      "type": "custom",
      "main": "src/backend/index.ts",
      "candid": "src/backend/index.did",
      "build": "npx azle backend",
      "wasm": ".azle/backend/backend.wasm",
      "gzip": true,
      "assets": [["src/frontend/dist", "dist"]],
      "build_assets": "npm run build"
    },
    "install-dfx-extension": {
      "type": "custom",
      "main": "src/backend/index.ts",
      "build": "npx azle install-dfx-extension",
      "candid": "src/backend/index.did",
      "wasm": ".azle/install-dfx-extension/install-dfx-extension.wasm"
    }
  }
}
